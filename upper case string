#include<stdio.h>
#include<string.h>
int main()
{
    char name[100];
    int i;
    puts("enter string");
    gets(name);
    for(i=0;name[i]!='\0';i++)
    {
        if(name[i]>65&&name[i]<=90)
        putchar(name[i]+32);
        else if(name[i]>=97&&name[i]<=122)
        putchar(name[i]-32);
        else
        putchar(name[i]);
    }
}
    
